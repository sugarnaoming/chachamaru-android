<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.sugarnaoming.chachamaru">

  <uses-permission android:name="android.permission.INTERNET" />

  <application
    android:allowBackup="true"
    android:icon="@drawable/ic_chachamaru_icon"
    android:label="@string/app_name"
    android:roundIcon="@drawable/ic_chachamaru_icon"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity
      android:name=".MainActivity"
      android:label="@string/app_name"
      android:screenOrientation="portrait"
      android:theme="@style/ChachaTheme">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
      android:name=".GroupAddActivity"
      android:screenOrientation="portrait" />
    <activity
      android:name=".GroupEditActivity"
      android:screenOrientation="portrait" />
    <activity
      android:name=".TabAddActivity"
      android:screenOrientation="portrait" />
    <activity
      android:name=".TabEditActivity"
      android:screenOrientation="portrait" />
    <activity android:name=".AboutThisAppActivity"></activity>
  </application>

</manifest>